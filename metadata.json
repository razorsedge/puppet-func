{
  "name": "razorsedge-func",
  "version": "1.2.0",
  "author": "razorsedge",
  "summary": "A secure, scriptable remote control framework and API.  Func allows for running commands on remote systems in a secure way, like SSH, but offers several improvements.",
  "license": "Apache License, Version 2.0",
  "project_page": "https://github.com/razorsedge/puppet-func",
  "issues_url": "https://github.com/razorsedge/puppet-func/issues",
  "source": "https://github.com/razorsedge/puppet-func.git",
  "tags": [ "RHEL", "rhel", "OEL", "CentOS", "func", "certmaster", "fedora" ],
  "dependencies": [
    {
      "name": "razorsedge/certmaster",
      "version_requirement": ">=1.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=2.1.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "5", "6" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "5", "6" ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [ "5", "6" ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [ "16", "17", "18", "19", "20" ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": ">=2.7.20 <4.0.0"
    }
  ]
}
